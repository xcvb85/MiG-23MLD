<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <fdm>
        <jsbsim>
            <fcs>
                <wing-sweep-cmd type="double">1</wing-sweep-cmd>
                <dogtooth type="int">1</dogtooth>
                <vortex-generator type="int">1</vortex-generator>
                <cfd>1</cfd>
            </fcs>
            <systems>
                <chute>
                    <deploy type="double">0</deploy>
                    <deploy-rqst type="double">0</deploy-rqst>
                </chute>
            </systems>
        </jsbsim>
    </fdm>
    <controls>
        <gear>
            <brake-parking>1</brake-parking>
        </gear>
        <flight>
            <flaps>0</flaps>
            <flap-panel>
                <up type="bool">1</up>
                <takeoff type="bool">0</takeoff>
                <landing type="bool">0</landing>
            </flap-panel>
            <elevator-trim>0.00</elevator-trim>
            <chute_jettisoned type="int">0</chute_jettisoned>
        </flight>
    </controls>
    <instrumentation>
        <altimeter>
            <indicated-altitude-ft type="double">0</indicated-altitude-ft>
        </altimeter>
        <gps>
            <indicated-vertical-speed type="double">0</indicated-vertical-speed>
        </gps>
        <radar>
            <enabled type="bool">true</enabled>
            <iff type="bool">false</iff>
            <az-field type="int">120</az-field>
            <ho-field type="int">120</ho-field><!-- bars -->
            <az-field-std type="int">120</az-field-std>
            <ho-field-std type="int">120</ho-field-std><!-- bars -->
            <range type="double">160</range>
            <mode-hd-switch type="int">-1</mode-hd-switch>
            <range-selected type="double">40</range-selected>
            <display-rdr type="bool">true</display-rdr>
            <radar2-range type="int">40</radar2-range><!-- default range -->
            <radar2-range-std type="int">40</radar2-range-std>
            <radar-standby type="bool">true</radar-standby>
            <radar-standby-std type="bool">true</radar-standby-std>
            <symbols-enabled type="bool">true</symbols-enabled>
            <debug-mode type="bool">true</debug-mode>
            <serviceable type="bool">true</serviceable>
            <mode>
                <rws type="bool">true</rws>
                <tws-auto type="bool">false</tws-auto>
            </mode>
            <limited-select type="bool">true</limited-select>
            <radar-mode type="int">1</radar-mode>
            <ref-range type="double">70</ref-range>
            <ref-strength type="double">3.2</ref-strength>
            <!-- Source: GR1F-F16CJ-34-1, please dont mess with this -->
            <ranges n="0">
                <entry type="int">10</entry>
            </ranges>
            <ranges n="1">
                <entry type="int">20</entry>
            </ranges>
            <ranges n="2">
                <entry type="int">40</entry>
            </ranges>
            <ranges n="3">
                <entry type="int">80</entry>
            </ranges>
            <ranges n="4">
                <entry type="int">160</entry>
            </ranges>
            <rangesGM n="0">
                <entry type="int">10</entry>
            </rangesGM>
            <rangesGM n="1">
                <entry type="int">20</entry>
            </rangesGM>
            <rangesGM n="2">
                <entry type="int">40</entry>
            </rangesGM>
            <rangesGM n="3">
                <entry type="int">80</entry>
            </rangesGM>
            <rangesDGFT n="0">
                <entry type="int">10</entry>
            </rangesDGFT>
            <cycle-range type="bool">false</cycle-range>
            <scan_visibility_check_interval type="double">1</scan_visibility_check_interval>
        </radar>
        <radar2>
            <sweep-width-m type="double">0.0844</sweep-width-m>
            <range-width-m type="double">0.0846</range-width-m>
            <radius-ppi-display-m type="double">0.0846</radius-ppi-display-m>
            <hud-eye-dist-m type="double">0.766</hud-eye-dist-m>
            <hud-radius-m type="double">0.071</hud-radius-m>
            <hud-vertical-offset-m type="double">-0.025</hud-vertical-offset-m>
        </radar2>
        <rwr>
            <display-lighting-intens type="double">0.4</display-lighting-intens>
            <symbology-lighting-intens type="double">1</symbology-lighting-intens>
        </rwr>
        <iff>
            <channel_prop type="string">/instrumentation/iff/channel-selection</channel_prop>
            <power_prop type="string">/instrumentation/iff/power</power_prop>
            <power type="bool">true</power>
            <channel-selection type="int">1</channel-selection>
            <response type="bool">false</response>
        </iff>
        <ecm>
            <on-off type="bool">true</on-off>
        </ecm>
    </instrumentation>
</PropertyList>
