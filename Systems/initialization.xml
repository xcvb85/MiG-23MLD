            <elevator-trim>0.00</elevator-trim>
            <chute_jettisoned type="int">0</chute_jettisoned>
        </flight>
        <radar>
            <antennae-knob type="double">0</antennae-knob>
        </radar>
    </controls>
    <instrumentation>
        <marker-beacon>
            <serviceable type="bool">false</serviceable>
        </marker-beacon>
        <radar-altimeter>
            <serviceable type="bool">true</serviceable>
        </radar-altimeter>
    </instrumentation>
