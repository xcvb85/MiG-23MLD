<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <fdm>
        <jsbsim>
            <fcs>
                <canopy-cmd-norm type="double">1</canopy-cmd-norm>
                <wingsweep-cmd-norm type="double">1</wingsweep-cmd-norm>
                <dogtooth type="bool">0</dogtooth>
                <vortex-generator type="bool">0</vortex-generator>
                <cfd type="bool">0</cfd>
            </fcs>
            <systems>
                <chute>
                    <deploy type="double">0</deploy>
                    <deploy-rqst type="double">0</deploy-rqst>
