    <controls>
        <armament>
            <master-arm-switch type="int">0</master-arm-switch>
        </armament>
        <ejected>0</ejected>
        <gear>
            <brake-parking>1</brake-parking>
        </gear>
        <flight>
            <flaps>0</flaps>
            <flap-panel>
                <up type="bool">1</up>
                <takeoff type="bool">0</takeoff>
                <landing type="bool">0</landing>
