<?xml version="1.0" encoding="UTF-8"?>
<!-- Labels based on MiG-29 version -->
<PropertyList>
    <path>warnings3.gltf</path>
    <model>
        <path>warnings.ac</path>
    </model>
    <animation>
        <object-name>warnings</object-name>
        <type>select</type>
        <condition>
            <greater-than>
                <property>fdm/jsbsim/electric/sources/output-dc</property>
                <value>0</value>
            </greater-than>
        </condition>
    </animation>
    <animation>
        <object-name>L01</object-name>
        <type>pbr</type>
        <condition>
            <greater-than>
                <property>engines/engine/n1</property>
                <value>95</value>
            </greater-than>
        </condition>
        <emissive-factor>
            <red>10.0</red>
            <green>15.0</green>
            <blue>10.0</blue>
        </emissive-factor>
    </animation>
    <animation>
        <object-name>L02</object-name>
        <type>pbr</type>
        <condition>
            <property>engines/engine[0]/augmentation</property>
        </condition>
        <emissive-factor>
            <red>10.0</red>
            <green>15.0</green>
            <blue>10.0</blue>
        </emissive-factor>
    </animation>
    <animation>
        <object-name>L04</object-name>
        <object-name>L06</object-name>
        <type>pbr</type>
            <greater-than>
                <property>fdm/jsbsim/electric/output/pump</property>
                <value>18</value>
            </greater-than>
        <emissive-factor>
            <red>10.0</red>
            <green>15.0</green>
            <blue>10.0</blue>
        </emissive-factor>
    </animation>
    <animation>
        <object-name>L05</object-name>
        <type>pbr</type>
        <condition>
            <and>
                <equals>
                    <property>payload/armament/station/id-2-type</property>
                    <value>Droptank</value>
                </equals>
                <equals>
                    <property>payload/armament/station/id-2-count</property>
                    <value>1</value>
                </equals>
            </and>
        </condition>
        <emissive-factor>
            <red>10.0</red>
            <green>15.0</green>
            <blue>10.0</blue>
        </emissive-factor>
    </animation>
    <animation>
        <object-name>L09</object-name>
        <type>pbr</type>
        <condition>
            <less-than>
                <property>engines/engine/n1</property>
                <value>5</value>
            </less-than>
        </condition>
        <emissive-factor>
            <red>10.0</red>
            <green>5.0</green>
            <blue>5.0</blue>
        </emissive-factor>
    </animation>
    <animation>
        <object-name>L10</object-name>
        <type>pbr</type>
        <condition>
            <less-than>
                <property>fdm/jsbsim/electric/sources/ac-gen</property>
                <value>18</value>
            </less-than>
        </condition>
        <emissive-factor>
            <red>10.0</red>
            <green>5.0</green>
            <blue>5.0</blue>
        </emissive-factor>
    </animation>
</PropertyList>
