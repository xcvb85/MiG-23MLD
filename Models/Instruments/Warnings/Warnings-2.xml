<?xml version="1.0" encoding="UTF-8"?>
<!-- Labels based on MiG-29 version -->
<PropertyList>
    <path>warnings2.gltf</path>
    <model>
        <path>warnings.ac</path>
    </model>
    <animation>
        <object-name>warnings</object-name>
        <type>select</type>
        <condition>
            <greater-than>
                <property>fdm/jsbsim/electric/sources/output-dc</property>
                <value>0</value>
            </greater-than>
        </condition>
    </animation>
    <animation>
        <object-name>L06</object-name>
        <type>pbr</type>
        <condition>
            <property>fdm/jsbsim/warning-lights/L600</property>
        </condition>
        <emissive-factor>
            <red>10.0</red>
            <green>5.0</green>
            <blue>5.0</blue>
        </emissive-factor>
    </animation>
    <animation>
        <object-name>L07</object-name>
        <type>pbr</type>
        <condition>
            <and>
                <greater-than>
                    <property>orientation/alpha-deg</property>
                    <value>15</value>
                </greater-than>
                <greater-than>
                    <property>fdm/jsbsim/fcs/wing-sweep</property>
                    <value>0</value>
                </greater-than>
            </and>
        </condition>
        <emissive-factor>
            <red>10.0</red>
            <green>5.0</green>
            <blue>5.0</blue>
        </emissive-factor>
    </animation>
    <animation>
        <object-name>L08</object-name>
        <type>pbr</type>
        <condition>
            <less-than>
                <property>fdm/jsbsim/electric/sources/dc-gen</property>
                <value>18</value>
            </less-than>
        </condition>
        <emissive-factor>
            <red>10.0</red>
            <green>5.0</green>
            <blue>5.0</blue>
        </emissive-factor>
    </animation>
</PropertyList>
