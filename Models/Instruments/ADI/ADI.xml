<?xml version="1.0" encoding="UTF-8"?>
<!-- Instrument from MiG-21, textures from MiG-23 model -->
<PropertyList>
    <path>adi.ac</path>
    <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>glass</object-name>
    </effect>
    <effect>
        <inherits-from>Aircraft/MiG-23MLD/Models/Effects/instruments/lm1</inherits-from>
        <object-name>face</object-name>
    </effect>
    <effect>
        <inherits-from>Aircraft/MiG-23MLD/Models/Effects/instruments/lm2</inherits-from>
        <object-name>case</object-name>
        <object-name>pitchTape</object-name>
        <object-name>azimuthNeedle</object-name>
    </effect>
    <animation>
        <type>rotate</type>
        <object-name>azimuthNeedle</object-name>
        <property>orientation/roll-deg</property>
        <factor>-1.0</factor>
        <center>
            <x-m> 0.000</x-m>
            <y-m> 0.000</y-m>
            <z-m> 0.000</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>pitchTape</object-name>
        <property>orientation/pitch-deg</property>
        <factor>1.9</factor>
        <center>
            <x-m>-0.045</x-m>
            <y-m> 0.000</y-m>
            <z-m> 0.000</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
</PropertyList>
