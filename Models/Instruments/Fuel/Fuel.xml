<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <path>Fuel.ac</path>
    <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>glass</object-name>
    </effect>
    <effect>
        <inherits-from>Aircraft/MiG-23MLD/Models/Effects/instruments/instruments</inherits-from>
        <object-name>face</object-name>
        <object-name>needle</object-name>
    </effect>
    <animation>
        <type>rotate</type>
        <object-name>needle</object-name>
        <property>consumables/fuel/total-fuel-gal_us</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>-162</dep>
            </entry>
            <entry>
                <ind>2220</ind>
                <dep>164</dep>
            </entry>
        </interpolation>
        <center>
            <x-m> 0.0</x-m>
            <y-m> 0.0</y-m>
            <z-m> 0.0</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <!--
        <condition>
            <greater-than>
                <property>fdm/jsbsim/electric/output/instruments</property>
                <value>20</value>
            </greater-than>
        </condition>
        -->
    </animation>
</PropertyList>
