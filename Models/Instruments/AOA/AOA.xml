<?xml version="1.0" encoding="UTF-8"?>
<!-- Based on instrument from MiG-29 -->
<PropertyList>
    <path>UAP-7.ac</path>
    <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>glass</object-name>
    </effect>
    <effect>
        <inherits-from>Aircraft/MiG-23MLD/Models/Effects/instruments/lm1</inherits-from>
        <object-name>face</object-name>
    </effect>
    <effect>
        <inherits-from>Aircraft/MiG-23MLD/Models/Effects/instruments/lm2</inherits-from>
        <object-name>needleA</object-name>
        <object-name>needleG</object-name>
    </effect>
    <animation>
        <type>rotate</type>
        <object-name>needleA</object-name>
        <property>orientation/alpha-deg</property>
        <interpolation>
            <entry>
                <ind>-5</ind>
                <dep>-23</dep>
            </entry>
            <entry>
                <ind>30</ind>
                <dep>135</dep>
            </entry>
        </interpolation>
        <condition>
            <not>
                <property>gear/gear/wow</property>
            </not>
        </condition>
        <axis>
            <x>-1</x>
            <y> 0</y>
            <z> 0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>needleG</object-name>
        <property>/fdm/jsbsim/accelerations/Nz</property>
        <interpolation>
            <entry>
                <ind>-2</ind>
                <dep>25</dep>
            </entry>
            <entry>
                <ind>10</ind>
                <dep>-125</dep>
            </entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
</PropertyList>
