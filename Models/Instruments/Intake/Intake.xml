<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <path>UPES.ac</path>
    <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>glass</object-name>
    </effect>
    <effect>
        <inherits-from>Aircraft/MiG-23MLD/Models/Effects/instruments/lm1</inherits-from>
        <object-name>face</object-name>
    </effect>
    <effect>
        <inherits-from>Aircraft/MiG-23MLD/Models/Effects/instruments/lm2</inherits-from>
        <object-name>needle1</object-name>
        <object-name>needle2</object-name>
    </effect>
    <animation>
        <type>knob</type>
        <object-name>knob</object-name>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>intake-bug</tooltip-id>
                <label>Air Intake Command</label>
            </binding>
        </hovered>
        <property>fdm/jsbsim/fcs/intake1-cmd-norm</property>
        <factor>-50</factor>
        <center>
            <x-m> 0.000</x-m>
            <y-m>-0.015</y-m>
            <z-m>-0.015</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>fdm/jsbsim/fcs/intake1-cmd-norm</property>
                <factor>0.02</factor>
                <min>0</min>
                <max>1</max>                
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>needle1</object-name>
        <property>fdm/jsbsim/fcs/intake1-cmd-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>-300.0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>needle2</object-name>
        <property>fdm/jsbsim/fcs/intake1-pos-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>-300.0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
</PropertyList>
