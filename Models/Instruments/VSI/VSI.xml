<?xml version="1.0" encoding="UTF-8"?>
<!-- Instrument from MiG-29 -->
<PropertyList>
    <path>DA-200.ac</path>
    <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>glass</object-name>
    </effect>
    <effect>
        <inherits-from>Aircraft/MiG-23MLD/Models/Effects/instruments/lm1</inherits-from>
        <object-name>vspeedFace</object-name>
    </effect>
    <effect>
        <inherits-from>Aircraft/MiG-23MLD/Models/Effects/instruments/lm2</inherits-from>
        <object-name>vspeedneedle</object-name>
        <object-name>turnneedle</object-name>
    </effect>
    <animation>
        <type>rotate</type>
        <object-name>vspeedneedle</object-name>
        <property>velocities/vertical-speed-fps</property>
        <interpolation>
            <entry><ind>656.168</ind><dep>136.0</dep></entry>
            <entry><ind>-65.6168</ind><dep>-68.0</dep></entry>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>65.6168</ind><dep>68.0</dep></entry>
            <entry><ind>656.168</ind><dep>136.0</dep></entry>
        </interpolation>
        <center>
            <x-m> 0.0</x-m>
            <y-m> 0.0</y-m>
            <z-m> 0.0</z-m>
        </center>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>turnneedle</object-name>
        <property>orientation/roll-deg</property>
        <interpolation>
            <entry><ind>45.0</ind><dep>25.0</dep></entry>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>-45.0</ind><dep>-25.0</dep></entry>
        </interpolation>
        <center>
            <x-m> 0.0</x-m>
            <y-m> 0.0</y-m>
            <z-m>-0.028</z-m>
        </center>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>vspeedBall</object-name>
        <property>instrumentation/slip-skid-ball/indicated-slip-skid</property>
        <interpolation>
            <entry><ind>3.0</ind><dep>0.0115</dep></entry>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>-3.0</ind><dep>-0.0115</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0.0044</z>
        </axis>
    </animation>
    <!-- Label text -->
    <text>
        <name>LabelText</name>
        <type type="string">literal</type>
        <text type="string">DA-200 Combo Indicator</text>
        <draw-text type="bool">true</draw-text>
        <font>Helvetica.txf</font>
        <character-size type="double">0.012</character-size>
        <character-aspect-ratio type="double">1.0</character-aspect-ratio>
        <axis-alignment>yz-plane</axis-alignment>
        <max-height>0.2</max-height>
        <max-width>0.07</max-width>
        <font-resolution>
            <width type="int">32</width>
            <height type="int">32</height>
        </font-resolution>
        <alignment>center-center</alignment>
        <offsets>
            <x-m> 0.01</x-m>
            <y-m> 0.0 </y-m>
            <z-m> 0.0 </z-m>
        </offsets>
    </text>
    <animation>
        <type>material</type>
        <object-name>LabelText</object-name>
        <emission>
            <red>1.0</red>
            <green>0.8</green>
            <blue>0.0</blue>
        </emission>
    </animation>
    <animation>
        <type>select</type>
        <object-name>LabelText</object-name>
        <condition>
            <property>/sim/panel-hotspots</property>
        </condition>
    </animation>
</PropertyList>
