    <model>
        <name>landing light1</name>
        <path>Aircraft/MiG-23MLD/Models/Effects/lights/landing-light.xml</path>
        <offsets>
            <x-m>-3.35</x-m>
            <y-m>-0.73</y-m>
            <z-m>-0.72</z-m>
            <heading-deg>5</heading-deg>
